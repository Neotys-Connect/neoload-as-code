{
  "name": "geolookup",
  "version": "1.0.0",
  "description": "A simple API to lookup GPS coordinates based on city name",
  "main": "index.js",
  "scripts": {
    "redis-init": "docker run -d --name redis -p 6379:6379 redis",
    "redis-live": "docker run -d --name redislive -p 6379:6379 paulsbruce/redislive",
    "redis": "node index.js --redis=6379",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
