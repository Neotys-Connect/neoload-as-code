  "bugs": {
    "url": "https://github.com/paulsbruce/neoload-as-code/issues"
  },
  "homepage": "https://github.com/paulsbruce/neoload-as-code#readme",
  "dependencies": {
    "express": "^4.17.1",
    "redis": "^2.8.0",
    "redis-delete-wildcard": "0.0.3",
    "request": "^2.88.0",
    "yargs": "^13.2.4"
  }
}
